<Window x:Class="MorphicWin.MorphicConfigurator"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MorphicWin"
        mc:Ignorable="d"
        Title="MorphicConfigurator" Height="450" Width="800">
    <Grid>
        <Button x:Name="createUserButton" Content="Create Test User" HorizontalAlignment="Center" VerticalAlignment="Center" Click="CreateTestUser"/>
        <Button x:Name="clearUserButton" Content="Clear Test User" HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="Hidden" Click="ClearTestUser"/>
    </Grid>
</Window>
