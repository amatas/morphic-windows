<StackPanel x:Class="MorphicWin.CreateAccountPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MorphicWin"
        mc:Ignorable="d" Height="450" Width="800">
    <TextBox x:Name="UsernameField" AcceptsReturn="False" AcceptsTab="False" MaxLines="1" TextWrapping="NoWrap" TextChanged="UsernameField_TextChanged" />
    <PasswordBox x:Name="PasswordField" PasswordChanged="PasswordField_PasswordChanged"/>
    <Button x:Name="SubmitButton">Create Account</Button>
</StackPanel>
