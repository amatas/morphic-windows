<UserControl x:Class="Morphic.ManualTester.SolutionHeader"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Morphic.ManualTester"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TreeViewItem Name="ControlStack" Header="SOLUTION" FontSize="20" FontWeight="Bold" HorizontalAlignment="Stretch" TreeViewItem.Expanded="ControlStack_Expanded" TreeViewItem.Collapsed="ControlStack_Collapsed"/>
    </Grid>
</UserControl>
